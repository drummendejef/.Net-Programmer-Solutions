<Window x:Class="Multithreading.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Multithreading"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <local:TellerTextBox x:Name="tellerTextBox" Margin="60,114,60,0" TextWrapping="Wrap" Text="TellerTextBox" VerticalAlignment="Top"/>
        <local:TellerTextBox x:Name="tellerTextBox1" Margin="60,114,60,0" TextWrapping="Wrap" Text="TellerTextBox" VerticalAlignment="Top" Grid.Column="1"/>
        <Button x:Name="button" Content="Button" Margin="60,145,60,0" VerticalAlignment="Top" Click="button_Click" />
        <Button x:Name="button1" Content="Button" Grid.Column="1" Margin="60,145,60,0" VerticalAlignment="Top" Click="button1_Click"/>
    </Grid>
</Window>

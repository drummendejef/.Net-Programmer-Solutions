<Window x:Class="Tekenprogramma.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tekenprogramma"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" SizeChanged="Window_SizeChanged">
    <Grid>
        <Canvas x:Name="canvas" Margin="10,10,10,37"/>
        <TextBox x:Name="textBoxFormule" Margin="10,0" TextWrapping="Wrap" Text="{Binding formule}" Height="23" VerticalAlignment="Bottom" />
    </Grid>
</Window>
